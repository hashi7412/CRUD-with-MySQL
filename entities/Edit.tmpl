{{ define "Edit" }}
  {{ template "Header" }}
    {{ template "Menu" }} 
   <h2>Edit Entity fields</h2>  
    <form method="POST" action="updateentities">
      <input type="hidden" name="uEntityID" value="{{ .EntityID }}" />
        
     
     
      <label>EntityName </label><input type="text" name="EntityName" value="{{ .EntityName}}"  /><br />
        
     
     
      <label>EntityType </label><input type="text" name="EntityType" value="{{ .EntityType}}"  /><br />
        
     
      <label> CreatedAt </label><input type="datetime-local" name="CreatedAt" value="{{ .CreatedAt}}"  /><br />
     
        
     
      <label> UpdatedAt </label><input type="datetime-local" name="UpdatedAt" value="{{ .UpdatedAt}}"  /><br />
     
      
      <input type="submit" value="Save user" class="btn btn-secondary" />
    </form><br />    
  {{ template "Footer" }}
{{ end }}
