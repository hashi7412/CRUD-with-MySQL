{{ define "Edit" }}
  {{ template "Header" }}
    {{ template "Menu" }} 
   <h2>Edit Entity fields</h2>  
    <form method="POST" action="updateintents">
      <input type="hidden" name="uIntentID" value="{{ .IntentID }}" />
        
     
     
      <label>IntentName </label><input type="text" name="IntentName" value="{{ .IntentName}}"  /><br />
        
     
     
      <label>Description </label><input type="text" name="Description" value="{{ .Description}}"  /><br />
        
     
      <label> CreatedAt </label><input type="datetime-local" name="CreatedAt" value="{{ .CreatedAt}}"  /><br />
     
        
     
      <label> UpdatedAt </label><input type="datetime-local" name="UpdatedAt" value="{{ .UpdatedAt}}"  /><br />
     
      
      <input type="submit" value="Save user" class="btn btn-secondary" />
    </form><br />    
  {{ template "Footer" }}
{{ end }}
